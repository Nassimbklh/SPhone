<svg width="400" height="800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="phoneGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1d4ed8;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Phone body -->
  <rect x="50" y="50" width="300" height="700" rx="40" fill="url(#phoneGradient)" stroke="#1e3a8a" stroke-width="4"/>

  <!-- Screen -->
  <rect x="70" y="100" width="260" height="550" rx="20" fill="#0f172a"/>

  <!-- Notch -->
  <rect x="150" y="70" width="100" height="25" rx="12" fill="#1e3a8a"/>

  <!-- Home button -->
  <circle cx="200" cy="680" r="25" fill="none" stroke="#1e3a8a" stroke-width="3"/>

  <!-- Camera -->
  <circle cx="200" cy="82" r="8" fill="#374151"/>
</svg>
